@{
    ViewData["Title"] = "Mis Conocimientos";
}
<h1>@ViewData["Title"]</h1>

<div class="container">
    <div class="table-responsive">
    <table class="table table-bordered   table-striped" style="margin-top:100px">
      <thead class="table__head">
        <tr class="winner__table">
          <th><i class="fa fa-user" aria-hidden="true"></i>Habilidad</th>
          <th><i class="fa fa-map-marker" aria-hidden="true"></i> Nivel</th>
        </tr>
      </thead>
      <tbody>
        <tr class="winner__table">
          <td>HTML/CSS</td>
          <td><div class="progress">
              <div class="progress-bar bg-info" role="progressbar" style="width: 85%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">85%</div>
            </div></td>          
        </tr>

        <tr class="winner__table">
          <td>JavaScript</td>
          <td><div class="progress">
              <div class="progress-bar bg-success" role="progressbar" style="width: 75%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">75%</div>
            </div></td>          
        </tr>

         <tr class="winner__table">
          <td>XML</td>
          <td><div class="progress">
              <div class="progress-bar bg-danger" role="progressbar" style="width: 10%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">10%</div>
            </div></td>          
        </tr>

        <tr class="winner__table">
          <td>Java</td>
          <td><div class="progress">
              <div class="progress-bar bg-warning" role="progressbar" style="width: 45%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">45%</div>
            </div></td>          
        </tr>

        <tr class="winner__table">
          <td>C#</td>
          <td><div class="progress">
              <div class="progress-bar bg-success" role="progressbar" style="width: 70%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">70%</div>
            </div></td>          
        </tr>

        <tr class="winner__table">
          <td>PHP</td>
          <td><div class="progress">
              <div class="progress-bar bg-info" role="progressbar" style="width: 80%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">80%</div>
            </div></td>          
        </tr>

        <tr class="winner__table">
          <td>ASP.net</td>
          <td><div class="progress">
              <div class="progress-bar bg-success" role="progressbar" style="width: 75%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">75%</div>
            </div></td>          
        </tr>

      </tbody>
    </table>
    </div>
     <img id="codigosprogramacion" style="padding-top: 5%; padding-left: 20%; width: 80%; height: 80%;" src="~/img/codigos.jpg"/>
  </div>